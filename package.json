{
  "name": "@unruggable/gateways",
  "version": "0.0.6",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ethereum",
    "evm",
    "gateway",
    "ccip",
    "proof",
    "unruggable"
  ],
  "type": "module",
  "scripts": {
    "check": "tsc -p . --noEmit",
    "forge": "forge build --force",
    "serve": "bun test/serve.ts",
    "test": "bun test --timeout 25000",
    "test-components": "bun test test/components/* --timeout 25000",
    "test-gateways": "bun test test/gateway/* --timeout 25000",
    "lint": "eslint . --ext .ts",
    "build": "bun build-dist.ts"
  },
  "sideEffects": false,
  "exports": {
    "types": "./dist/types/index.d.ts",
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.mjs"
  },
  "files": [
    "dist/",
    "src/",
    "contracts/DataFetcher.sol",
    "contracts/DataFetchTarget.sol",
    "contracts/ProtocolData.sol",
    "contracts/IDataProver.sol",
    "contracts/IDataProofVerifier.sol"
  ],
  "devDependencies": {
    "@adraffy/blocksmith": "^0.0.28",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@ensdomains/ccip-read-router": "^0.0.5",
    "@resolverworks/ezccip": "^0.0.14",
    "ethers": "^6.13.1",
    "viem": "^2.21.2"
  },
  "peerDependencies": {
    "typescript": "^5.5.2"
  }
}
